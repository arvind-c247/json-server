{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js",
      "methods": [
        "GET",
        "POST",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ]
    }
  ]
}