{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includefiles": [
          "db.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(_*)",
      "dest": "server.js",
      "methods": [
        "GET",
        "POST",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ]
    }
  ]
}